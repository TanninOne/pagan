{
  "name": "node-pagan",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "configure": "cmake-js configure",
    "install": "conan install . && cmake-js compile",
    "build": "conan install . --settings=build_type=Release && cmake-js compile --config Release --CDCMAKE_TOOLCHAIN_FILE=\"./generators/conan_toolchain.cmake\"",
    "build_dbg": "conan install . --settings=build_type=Debug && cmake-js compile --config Debug --CDCMAKE_TOOLCHAIN_FILE=\"./generators/conan_toolchain.cmake\"",
    "build_test": "cmake-js compile --config Debug -T tests",
    "test": "yarn run build_test && ./build/Debug/tests.exe"
  },
  "dependencies": {
    "cmake-js": "^7.3.0",
    "node-addon-api": "*"
  },
  "binary": {
    "napi_versions": [
      4
    ]
  }
}
